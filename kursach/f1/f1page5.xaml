<Window x:Class="kursach.f1.f1page5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kursach.f1"
        mc:Ignorable="d"
        Title="f1page5" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="text" FontWeight="Bold" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,50,0,0" Width="700" Height="250" BorderThickness="0"/>
        <Button Click="Button_Click" Content="Выйти" HorizontalAlignment="Center" VerticalAlignment="Top" Height="25" Width="100" Margin="0,10,650,0"/>
        <Button Click="Button_Click_2" Content="Предыдущая страница" HorizontalAlignment="Center" VerticalAlignment="Top" Height="25" Width="150" Margin="0,370,600,0"/>
        <Button Click="Button_Click_3" x:Name="save" Content="Сохранить изменения" HorizontalAlignment="Center" VerticalAlignment="Top" Height="25" Width="150" Margin="0,370,0,0"/>
        <Image x:Name="IMG" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,100,0,0" Width="400" Opacity="0.2"/>
    </Grid>
</Window>
